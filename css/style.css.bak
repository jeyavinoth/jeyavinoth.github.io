/* rooms column */ 
.rooms-column-container { 
  text-align: center;
  height: 500px;
}

/* room image */
.room-image-wrapper {
  padding: 10px; 
}

/* image of the rooms */ 
.room-image {
  width: 250px; 
  height: 150px; 
  border: solid; 
  border-width: thick;
  border-color: darkgrey;
  padding: 1px; 
  -webkit-transition: all 0.4s ease;
  -moz-transition: all 0.4s ease;
  -o-transition: all 0.4s ease;
  -ms-transition: all 0.4s ease;
  transition: all 0.4s ease;
}


/* hover animations for rooms */ 
.room-image:hover {

  border-color: white; 

  /* animation settings */ 
  -moz-transform: scale(1.1); 
  -ms-transform: scale(1.1); 
  -o-transform: scale(1.1); 
  -webkit-transform: scale(1.1); 
  transform: scale(1.1); 

  -webkit-transition: all 0.4s ease;
  -moz-transition: all 0.4s ease;
  -o-transition: all 0.4s ease;
  -ms-transition: all 0.4s ease;
  transition: all 0.4s ease;
}


